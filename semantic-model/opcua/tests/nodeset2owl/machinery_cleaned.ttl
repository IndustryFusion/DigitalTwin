    base:hasBrowseName "OperationCycleCounter" ;
    base:hasDatatype opcua:UInteger ;
    base:hasDescription "OperationCycleCounter is counting the times the component switches from not performing an activity to performing an activity. For example, each time a valve starts moving, is counted. This value shall only increase during the lifetime of the component and shall not be reset when the component is restarted." ;
