            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNode ;
            sh:path test:hasB3 ;
            sh:property [ sh:class test:BType ;
                    sh:maxCount 1 ;
                    sh:minCount 1 ;
                    sh:nodeKind sh:IRI ;
                    sh:path ngsi-ld:hasObject ] ;
            base:hasReferenceType opcua:HasComponent ],
        [ a base:PeerRelationship ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNode ;
            sh:path test:X ;
