                                sh:minCount 1 ;
                                sh:nodeKind sh:Literal ;
                                sh:path ngsi-ld:hasValue ] ] [ sh:property [ sh:maxCount 1 ;
                                sh:minCount 1 ;
                                sh:or ( [ sh:nodeKind sh:BlankNode ;
                                            sh:property [ sh:datatype xsd:double ;
