            base:hasReferenceType opcua:HasComponent ],
        [ sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNode ;
            sh:or ( [ sh:property [ sh:datatype xsd:double ;
